@article{gawron_impact_2021,
	title = {Impact of {Health} {Insurance}, {Poverty}, and {Comorbidities} on {Colorectal} {Cancer} {Screening}: {Insights} from the {Medical} {Expenditure} {Panel} {Survey}},
