	title = {Heart {Failure} and the {Affordable} {Care} {Act}: {Past}, {Present}, and {Future}},
	volume = {7},
	issn = {2213-1779},
	shorttitle = {Heart {Failure} and the {Affordable} {Care} {Act}},
	url = {https://www.sciencedirect.com/science/article/pii/S2213177919303373},
	doi = {10.1016/j.jchf.2019.04.021},
	abstract = {The Affordable Care Act (ACA) and other major health care legislative acts have had an important impact on the care of heart failure patients in the United States. The main effects of the ACA include regulation of the health insurance industry, expansion of access to health care, and health care delivery system reform, which included the creation of several alternative payment models. Particular components of the ACA, such as the elimination of annual and lifetime caps on spending, Medicaid expansion, and the individual and employer mandate, could have positive effects for heart failure patients. However, the benefits of value-based and alternative payment models such as the Hospital Readmissions Reduction Program and bundled payment programs for heart failure outcomes are less clear, and controversy exists regarding whether some of these programs may even worsen outcomes. As the population ages and the prevalence of heart failure continues to rise, this syndrome will likely remain a key clinical focus for policymakers. Therefore, heart failure clinicians should be aware of how legislation affects clinical practice and be prepared to adapt to continued changes in health policy.},
	number = {9},
	urldate = {2023-10-11},
	journal = {JACC: Heart Failure},
	author = {Wolfe, Jonathan D. and Joynt Maddox, Karen E.},
	month = sep,
	year = {2019},
	keywords = {Affordable Care Act, health policy, heart failure, notion},
	pages = {737--745},
}

