@article{bandi_updated_2021,
	title = {Updated {Review} of {Major} {Cancer} {Risk} {Factors} and {Screening} {Test} {Use} in the {United} {States} in 2018 and 2019, with a {Focus} on {Smoking} {Cessation}},
	volume = {30},
	issn = {1055-9965},
	url = {https://doi.org/10.1158/1055-9965.EPI-20-1754},
	doi = {10.1158/1055-9965.EPI-20-1754},
	abstract = {Cancer prevention and early detection efforts are central to reducing cancer burden. Herein, we present estimates of cancer risk factors and screening tests in 2018 and 2019 among US adults, with a focus on smoking cessation. Cigarette smoking reached a historic low in 2019 (14.2\%) partly because 61.7\% (54.9 million) of all persons who had ever smoked had quit. Yet, the quit ratio was \&lt;45\% among lower-income, uninsured, and Medicaid-insured persons, and was \&lt;55\% among Black, American Indian/Alaska Native, lower-educated, lesbian, gay or bisexual, and recent immigrant persons, and in 12 of 17 Southern states. Obesity levels remain high (2017–2018: 42.4\%) and were disproportionately higher among Black (56.9\%) and Hispanic (43.7\%) women. HPV vaccination in adolescents 13 to 17 years remains underutilized and over 40\% were not up-to-date in 2019. Cancer screening prevalence was suboptimal in 2018 (colorectal cancer ≥50 years: 65.6\%; breast ≥45 years: 63.2\%; cervical 21–65 years: 83.7\%), especially among uninsured adults (colorectal: 29.8\%; breast: 31.1\%). This snapshot of cancer prevention and early detection measures was mixed, and substantial racial/ethnic and socioeconomic disparities persisted. However, gains could be accelerated with targeted interventions to increase smoking cessation in under-resourced populations, stem the obesity epidemic, and improve screening and HPV vaccination coverage.},
	number = {7},
	urldate = {2023-08-28},
	journal = {Cancer Epidemiology, Biomarkers \& Prevention},
	author = {Bandi, Priti and Minihan, Adair K. and Siegel, Rebecca L. and Islami, Farhad and Nargis, Nigar and Jemal, Ahmedin and Fedewa, Stacey A.},
	month = jul,
