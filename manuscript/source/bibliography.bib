@article{chait_promoting_2018,
	title = {Promoting {Prevention} {Under} the {Affordable} {Care} {Act}},
	volume = {39},
	url = {https://doi.org/10.1146/annurev-publhealth-040617-013534},
	doi = {10.1146/annurev-publhealth-040617-013534},
	abstract = {The Affordable Care Act (ACA) of 2010 placed a substantial emphasis on public health and prevention. Subsequent research on its effects reveals some notable successes and some missteps and offers important lessons for future legislators. The ACA's Prevention and Public Health Fund, intended to give public health budgetary flexibility, provided crucial funding for public health services during the Great Recession but proved highly vulnerable to subsequent budget cuts. Several programs that aimed to increase strategic thinking and planning around public health at the state level have proven to be more enduring, suggesting that the convening authority of the federal government can be a powerful tool for progress, especially when buttressed by some funding. Most important, by expanding insurance and mandating a minimum level of coverage, the ACA both increased access to clinical preventive services and freed up local public health budgets to engage in population health activities.},
	number = {1},
	urldate = {2023-10-09},
	journal = {Annual Review of Public Health},
	author = {Chait, Nadia and Glied, Sherry},
	year = {2018},
	pmid = {29400992},
	note = {\_eprint: https://doi.org/10.1146/annurev-publhealth-040617-013534},
	keywords = {Affordable Care Act, capacity, incentives, prevention, public health, reform},
	pages = {507--524},
}

