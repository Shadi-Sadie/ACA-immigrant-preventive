@inproceedings{gupta_objective_2012,
	title = {Objective versus {Subjective} {Measures} of {Health} : {Systematic} differences , determinants and biases},
	shorttitle = {Objective versus {Subjective} {Measures} of {Health}},
	url = {https://www.semanticscholar.org/paper/Objective-versus-Subjective-Measures-of-Health-%3A-%2C-Gupta-Adam/7174b7014941dc2bab59c1125b2ab4183c8b658c},
	abstract = {This study investigates the extent, predictors and consequences of disparities between self-reported and measured obesity in a young population. Past research has shown the relevance of both self-reported and measured health as the outcome of interest or as a key predictor of other important variables like labor force participation, retirement decisions of workers etc. These studies however have produced mixed results of the determinants of health and its role as a predictor of other economic outcomes, with results frequently differing by the choice of health measure used measured versus selfreported. Although objective/measured and subjective/self-reported health should essentially reflect the same thing there is ample evidence in past literature of large disparities between the two health measures in even very high quality surveys. These differences raise several questions: What is the extent of the disparity between objective and subjective health? Are there any consistent predictors of these differences that can be used to reconcile these differences? What are the consequences of the disparity between self-reported and measured health for economic models? Focussing on hypertension and obesity, which together with their co-morbidities are the two biggest ‘killers’ of our time, this study attempts to answer precisely these questions. Using a nationally representative sample of Americans aged 24-32 years from the most recent 2008 wave of the National Longitudinal Survey of Adolescent Health (Add Health)we find statistically significant differences in incidence of both hypertension and obesity ∗Indiana University-Purdue University Indianapolis, 524 Cavanaugh Hall, Department of Economics, IUPUI, Indianapolis, IN 46202-5147. sugupta@iupui.edu. †Northwestern University ‡Northwestern University},
	urldate = {2023-10-12},
	author = {Gupta, Sumedha and Adam, E. and McDade, T.},
	year = {2012},
	keywords = {notion},
}

@article{barnes_changes_2023,
	title = {Changes in cancer mortality after {Medicaid} expansion and the role of stage at diagnosis},
	volume = {115},
	issn = {0027-8874},
	url = {https://doi.org/10.1093/jnci/djad094},
	doi = {10.1093/jnci/djad094},
	abstract = {Medicaid expansion is associated with improved survival following cancer diagnosis. However, little research has assessed how changes in cancer stage may mediate improved cancer mortality or how expansion may have decreased population-level cancer mortality rates.Nationwide state-level cancer data from 2001 to 2019 for individuals ages 20-64 years were obtained from the combined Surveillance, Epidemiology, and End Results National Program of Cancer Registries (incidence) and the National Center for Health Statistics (mortality) databases. We estimated changes in distant stage cancer incidence and cancer mortality rates from pre- to post-2014 in expansion vs nonexpansion states using generalized estimating equations with robust standard errors. Mediation analyses were used to assess whether distant stage cancer incidence mediated changes in cancer mortality.There were 17 370 state-level observations. For all cancers combined, there were Medicaid expansion–associated decreases in distant stage cancer incidence (adjusted odds ratio = 0.967, 95\% confidence interval  = 0.943 to 0.992; P = .01) and cancer mortality (adjusted odds ratio = 0.965, 95\% confidence interval = 0.936 to 0.995; P = .022). This translates to 2591 averted distant stage cancer diagnoses and 1616 averted cancer deaths in the Medicaid expansion states. Distant stage cancer incidence mediated 58.4\% of expansion-associated changes in cancer mortality overall (P = .008). By cancer site subgroups, there were expansion-associated decreases in breast, cervix, and liver cancer mortality.Medicaid expansion was associated with decreased distant stage cancer incidence and cancer mortality. Approximately 60\% of the expansion-associated changes in cancer mortality overall were mediated by distant stage diagnoses.},
	number = {8},
	urldate = {2023-10-11},
	journal = {JNCI: Journal of the National Cancer Institute},
	author = {Barnes, Justin M and Johnson, Kimberly J and Osazuwa-Peters, Nosayaba and Yabroff, K Robin and Chino, Fumiko},
	month = aug,
	year = {2023},
	keywords = {\#ACA, \#Health/Cancer, \#Health/Outcome, notion},
	pages = {962--970},
}

@article{dondero_immigrant_2020,
	title = {Immigrant policies as health policies: {State} immigrant policy climates and health provider visits among {U}.{S}. immigrants},
	volume = {10},
	issn = {2352-8273},
	shorttitle = {Immigrant policies as health policies},
	doi = {10.1016/j.ssmph.2020.100559},
	abstract = {The geographic dispersion of the U.S. immigrant population has occurred alongside a dramatic increase in state-level immigration laws that has unfolded unevenly across states, creating markedly different state immigrant policy climates. Although not all such laws are health-related, they have potential implications for immigrants' health care utilization. Using data from the 2014 Survey of Income and Program Participation, we leverage the geographic variation in the restrictiveness of state immigrant policy climates to examine the association between state-level immigrant policies and health provider visits-a fundamental indicator of health care utilization-among immigrant adults. Results indicate that restrictive immigrant policy climates exacerbate nativity gaps in health provider visits among working-age adults and, to a lesser extent, among older adults. Our findings suggest that even immigrant policies not directly related to health have consequences for immigrants' health care utilization.},
	language = {eng},
	journal = {SSM - population health},
